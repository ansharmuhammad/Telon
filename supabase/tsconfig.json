{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["esnext", "deno.ns"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "https://esm.sh/@supabase/functions-js/src/edge-runtime.d.ts"
    ]
  },
  "include": [
    "functions/**/*"
  ],
  "exclude": [
    "../node_modules"
  ]
}